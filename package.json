{
  "name": "dyn-immutable",
  "version": "0.2.0",
  "description": "Typescript compatible easy to use immutable modification library using expression ASTs to properly maintain immutability",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -Rf dist/* && cd src && tsc",
    "preversion": "npm test",
    "test": "mocha -r test/ts-node.es5 test/**/*.ts",
    "test-esnext": "mocha -r test/ts-node.esnext test/**/*.ts",
    "test-debug": "node --inspect-brk node_modules/mocha/bin/_mocha -r test/ts-node.es5 test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olee/dyn-immutable.git"
  },
  "keywords": [
    "immutable",
    "typescript",
    "ast"
  ],
  "author": "Bj√∂rn Zeutzheim <npm@bzeutzheim.de>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/olee/dyn-immutable/issues"
  },
  "homepage": "https://github.com/olee/dyn-immutable#readme",
  "dependencies": {
    "acorn-walk": "^6.1.1",
    "cherow": "^1.6.9",
    "deep-freeze-strict": "^1.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/deep-freeze-strict": "^1.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.21",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.1"
  }
}
